page {
  background: #f8f8f8;
  box-sizing: border-box;
}

.container {
  position: relative;
  height: 100vh;
  overflow: hidden;
}

.chatGroup {
  padding-bottom: 120rpx;
}

.chatWrap {
  padding: 32rpx 25rpx;
}

/* 对话框 */
.chatLabel {
  min-height: 90rpx;
  position: relative;
  margin-bottom: 20rpx;
  padding: 0 100rpx 0 66rpx;
  overflow: hidden;
  text-align: right;
}

.chatLabel .articleBox {
  display: inline-block;
  border-radius: 20rpx 0 20rpx 20rpx;
  background: #ffffff;
  padding: 20rpx 30rpx;
  overflow: hidden;
  font-size: 28rpx;
  color: rgb(0, 0, 0);
  line-height: 38rpx;
  min-height: 30rpx;
  text-align: justify;
  max-width: 420rpx;
  word-break: break-all;
}

.chatLabel .avatar {
  position: absolute;
  top: 0;
  right: 0;
  border-radius: 12rpx 12rpx 12rpx 12rpx;
  width: 80rpx;
  height: 80rpx;
  overflow: hidden;
}

.chatLabel.auto {
  padding: 5rpx 50rpx 0 100rpx;
  text-align: left;
}

.chatLabel.auto .articleBox {
  border-radius: 0 20rpx 20rpx 20rpx;
  background: rgb(250, 254, 255);
  color: #333;
}

.chatLabel.auto .avatar {
  right: auto;
  left: 0;
  border: 1rpx solid #eee;
  background: #fff;
}

/* 回复框 */
.chatFooterGroup {
  box-sizing: border-box;
  background: #fcfcfc;
  width: 100%;
  padding: 10rpx 10rpx 10rpx 10rpx;
  font-size: 24rpx;
  color: #999;
  line-height: 30rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 100;
  border-top: 0rpx solid #eee;
  overflow: hidden;
}

/* 隐藏的对话框 */
.chatFooterGroup .hiddeninput {
  display: none;
}

/* 自定义icon 说话 来自iconfont*/
@font-face {
  font-family: "iconfont";
  src: url('iconfont.eot?t=1613702543136');
  /* IE9 */
  src: url('iconfont.eot?t=1613702543136#iefix') format('embedded-opentype'),
    /* IE6-IE8 */
    url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
    url('iconfont.woff?t=1613702543136') format('woff'),
    url('iconfont.ttf?t=1613702543136') format('truetype'),
    /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
    url('iconfont.svg?t=1613702543136#iconfont') format('svg');
  /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.l-icon-speak:before {
  content: "\e663";
}

